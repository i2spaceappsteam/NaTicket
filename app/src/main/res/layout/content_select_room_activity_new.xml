<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:customfontdemo="http://schemas.android.com/apk/res-auto"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context="com.NaTicket.n.hotels.SelectRoomActivityNew"
    tools:showIn="@layout/activity_select_room_new"
    android:descendantFocusability="beforeDescendants"
    android:focusableInTouchMode="true">





            <android.support.v7.widget.RecyclerView
                android:id="@+id/roomList_view"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="1.0"
                android:scrollbars="vertical" />



    <com.NaTicket.n.custom.MyTextView
        android:id="@+id/proceedToBackTv"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Proceed To Book"
        android:padding="15dp"
        android:gravity="center"
        android:background="@color/colorAccent"
        android:textColor="@android:color/white"
        android:textSize="17sp"
        customfontdemo:fontName="@string/custom_font" />

</LinearLayout>
